			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#6598f1"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#ffba39"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#ff4949"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#d198fa"
			}
		},
		{
			"scope": "keyword.other.DML.sql",
			"settings": {
				"foreground": "#98c3ff"
			}
		},
		{
			"scope": "keyword.other.unit",
