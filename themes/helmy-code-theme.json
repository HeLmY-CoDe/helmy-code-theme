				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant",
				"constant.character",
				"constant.other",
				"variable.parameter"
			],
			"settings": {
				"foreground": "#ffb964"
			}
		},
		{
			"scope": [
				"markup.underline"
			],
			"settings": {
				"foreground": "#ffb964",
				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"constant.other.reference.link.markdown",
				"markup.changed.git_gutter"
			],
			"settings": {
				"foreground": "#00e0f0"
			}
		},
		{
			"scope": [
				"variable.language.fenced.markdown",
