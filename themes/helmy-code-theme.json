			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#ff4949"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#bc6bf7"
			}
		},
		{
			"scope": "keyword.other.DML.sql",
			"settings": {
				"foreground": "#9bc3ff"
			}
		},
		{
			"scope": "keyword.other.unit",
