				"markup.raw.block"
			],
			"settings": {
				"foreground": "#d1c2ff",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"meta"
			],
			"settings": {
				"foreground": "#ddeeff"
			}
		},
		{
